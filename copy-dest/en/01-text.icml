					</Cell>
					<Cell Self="ufai1688i4" Name="0:3" RowSpan="1" ColumnSpan="1" CellType="TextTypeCell" TextTopInset="9" TextLeftInset="18" TextBottomInset="9" TextRightInset="13.5" ClipContentToTextCell="false" AppliedCellStyle="CellStyle/Cell" AppliedCellStylePriority="0" LeftEdgeStrokePriority="7" RightEdgeStrokePriority="8" TopEdgeStrokePriority="7" BottomEdgeStrokePriority="9">
						<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/TablePar">
