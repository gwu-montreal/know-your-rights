						<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/TablePar &gt; TableHeader">
							<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Default">
								<Content>Notice Period</Content>
							</CharacterStyleRange>
						</ParagraphStyleRange>
					</Cell>
					<Cell Self="ufai1688i0" Name="0:1" RowSpan="1" ColumnSpan="1" CellType="TextTypeCell" TextTopInset="9" TextLeftInset="18" TextBottomInset="9" TextRightInset="13.5" ClipContentToTextCell="false" AppliedCellStyle="CellStyle/Cell" AppliedCellStylePriority="0" LeftEdgeStrokePriority="3" RightEdgeStrokePriority="4" TopEdgeStrokePriority="3" BottomEdgeStrokePriority="5">
						<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/TablePar">
							<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Default">
								<Content>3 months - 1 year</Content>
							</CharacterStyleRange>
						</ParagraphStyleRange>
					</Cell>
					<Cell Self="ufai1688i1" Name="1:1" RowSpan="1" ColumnSpan="1" CellType="TextTypeCell" TextTopInset="9" TextLeftInset="18" TextBottomInset="9" TextRightInset="13.5" ClipContentToTextCell="false" AppliedCellStyle="CellStyle/Cell" AppliedCellStylePriority="0" LeftEdgeStrokePriority="4" RightEdgeStrokePriority="4" TopEdgeStrokePriority="4" BottomEdgeStrokePriority="6">
						<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/TablePar">
							<CharacterStyleRange AppliedCharacterStyle="CharacterStyle/Default">
								<Content>1 week</Content>
							</CharacterStyleRange>
						</ParagraphStyleRange>
					</Cell>
					<Cell Self="ufai1688i2" Name="0:2" RowSpan="1" ColumnSpan="1" CellType="TextTypeCell" TextTopInset="9" TextLeftInset="18" TextBottomInset="9" TextRightInset="13.5" ClipContentToTextCell="false" AppliedCellStyle="CellStyle/Cell" AppliedCellStylePriority="0" LeftEdgeStrokePriority="5" RightEdgeStrokePriority="6" TopEdgeStrokePriority="5" BottomEdgeStrokePriority="7">
						<ParagraphStyleRange AppliedParagraphStyle="ParagraphStyle/TablePar">
